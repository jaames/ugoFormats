{
  "name": "ugo-formats",
  "version": "1.0.0",
  "description": "JavaScript API for parsing and rendering the proprietary image and menu formats used by Flipnote Hatena",
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "build": "bili",
    "dev": "concurrently 'npm run serve' 'bili --watch --no-logUpdate'",
    "serve": "static -c 0 -a 0.0.0.0 -p 4000"
  },
  "devDependencies": {
    "bili": "^3.1.2",
    "concurrently": "^3.5.1",
    "node-static": "^0.7.10"
  },
  "keywords": [
    "flipnote",
    "studio",
    "hatena",
    "ugomemo",
    "decoder",
    "nintendo",
    "うごメモ",
    "ntft",
    "nbf",
    "npf",
    "tmb",
    "ugo",
    "jaames"
  ],
  "author": "James Daniel <james@rakujira.jp>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaames/ugo-formats.git"
  },
  "bugs": {
    "url": "https://github.com/jaames/ugo-formats/issues"
  }
}
